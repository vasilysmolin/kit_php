{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "ext-curl": "*",
        "ext-exif": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-gmp": "*",
        "ext-json": "*",
        "doctrine/dbal": "^2.6",
        "fakerphp/faker": "^1.16",
        "fruitcake/laravel-cors": "^3.0",
        "guzzlehttp/guzzle": "^7.4.1",
        "healey/robots": "^1.0",
        "illuminate/console": "^v9.18.0",
        "illuminate/support": "^v9.18.0",
        "intervention/image": "2.*",
        "intervention/imagecache": "^2.4",
        "khill/lavacharts": "^3.1",
        "laravel/breeze": "^1.9.4",
        "laravel/framework": "^9.0",
        "laravel/helpers": "^1.2",
        "laravel/scout": "^9.4.9",
        "laravel/tinker": "^2.7",
        "league/flysystem-aws-s3-v3": "^3.0.21",
        "maatwebsite/excel": "3.*",
        "phpmailer/phpmailer": "^6.0",
        "predis/predis": "^v2.0.0",
        "pusher/pusher-php-server": "^4.1",
        "slevomat/coding-standard": "^7.0",
        "spatie/enum": "3.*",
        "spatie/laravel-backup": "^8.1.2",
        "spatie/laravel-permission": "^5.1",
        "spiti/smsclient": "1.1.0",
        "symfony/translation": "^5.3",
        "tymon/jwt-auth": "dev-develop",
        "vearutop/php-obscene-censor-rus": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "barryvdh/laravel-ide-helper": "^2.8",
        "brianium/paratest": "^6.3",
        "deployer/recipes": "^6.2",
        "spatie/laravel-ignition": "^1.0",
        "filp/whoops": "~2.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^6.1",
        "nunomaduro/larastan": "^0.7.12",
        "phpunit/php-code-coverage": "^9.0",
        "phpunit/phpunit": "9.5.x-dev",
        "rregeer/phpunit-coverage-check": "^0.3.1",
        "squizlabs/php_codesniffer": "^3.6",
        "wapmorgan/morphos": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
