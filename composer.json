{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-curl": "*",
        "ext-exif": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-gmp": "*",
        "ext-json": "*",
        "algolia/algoliasearch-client-php": "^3.0",
        "algolia/scout-extended": "^1.18",
        "doctrine/dbal": "^2.6",
        "fakerphp/faker": "^1.16",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "healey/robots": "^1.0",
        "illuminate/console": "^8.0",
        "illuminate/support": "^v8.55.0",
        "intervention/image": "2.*",
        "intervention/imagecache": "^2.4",
        "khill/lavacharts": "^3.1",
        "laravel-notification-channels/fcm": "~2.2.1",
        "laravel-notification-channels/webpush": "^5.1.1",
        "laravel/breeze": "^1.4",
        "laravel/framework": "^8.0",
        "laravel/helpers": "^1.2",
        "laravel/scout": "^8.6",
        "laravel/socialite": "5.*",
        "laravel/tinker": "^2.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "maatwebsite/excel": "3.1.26",
        "milon/barcode": "8.*",
        "nothingworks/blade-svg": "^1.1.2",
        "phpmailer/phpmailer": "^6.0",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^4.1",
        "spatie/enum": "3.*",
        "spatie/laravel-cookie-consent": "^2.12",
        "spatie/laravel-permission": "^5.1",
        "spiti/smsclient": "1.1.0",
        "symfony/translation": "^5.3",
        "tymon/jwt-auth": "1.0.2",
        "vearutop/php-obscene-censor-rus": "^1.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3",
        "barryvdh/laravel-debugbar": "^3.1",
        "barryvdh/laravel-ide-helper": "^2.8",
        "brianium/paratest": "^6.3",
        "deployer/recipes": "^6.2",
        "filp/whoops": "~2.0",
        "phpunit/php-code-coverage": "^9.0",
        "rregeer/phpunit-coverage-check": "^0.3.1",
        "squizlabs/php_codesniffer": "^3.6",
        "wapmorgan/morphos": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
